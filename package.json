{
  "name": "next-playground",
  "version": "0.1.0",
  "private": true,
  "description": "Go and create",
  "author": "Stefano Veltri <stefano@idrinkritalin.it> (@idrinkritalin)",
  "scripts": {
    "build": "next build",
    "commit": "git-cz",
    "dev": "next dev",
    "format": "prettier . --ext .js,.jsx,.ts,.tsx",
    "format:fix": "prettier . --ext .js,.jsx,.ts,.tsx --write",
    "lint": "next lint . --ext .js,.ts,.tsx",
    "prepare": "husky install",
    "start": "next start",
    "test": "jest",
    "test:e2e": "playwright test",
    "test:watch": "jest --watch",
    "type-check": "tsc"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "dependencies": {
    "next": "^13.1.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.6.0",
    "@playwright/test": "^1.30.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "autoprefixer": "^10.4.13",
    "cssnano": "^5.1.14",
    "dotenv": "^16.0.3",
    "eslint": "^8.33.0",
    "eslint-config-next": "13.1.6",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-jest": "^27.2.1",
    "git-cz": "^4.9.0",
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "lint-staged": "^13.0.3",
    "playwright": "^1.30.0",
    "postcss": "^8.4.21",
    "prettier": "2.8.3",
    "sort-package-json": "^2.3.0",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "typescript": "4.9.5"
  }
}